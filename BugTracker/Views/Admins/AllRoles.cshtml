@model List<string>
@{
    ViewBag.Title = "AllRoles";
}

<h2>AllRoles</h2>

<ul>
    @foreach (var role in Model)
    {
        <li>@role</li>
    }
</ul>

@if (ViewBag.Message != null)
{
    <script type="text/javascript">
            window.onload = function () {
                alert("@ViewBag.Message");
            };
    </script>
}