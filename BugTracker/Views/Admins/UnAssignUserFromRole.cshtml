
@{
    ViewBag.Title = "UnAssignUserFromRole";
}

<h2>UnAssign User From Role</h2>

@using (Html.BeginForm())
{
    <p>Select User @Html.DropDownList("UserId") </p>
    <br />
    <p>Select Role @Html.DropDownList("role") </p>
    <br />
    <button class="submitButton" type="submit">
        UnAssign User from the role
    </button>
    <br style="height:10px;"/>
    <button class="submitButton" type="button" style="margin-top:10px;">
        <p class="link">
            @Html.ActionLink("Back To Inedx Page", "Index", "Admins", null, new { style = "color: white;text-decoration:none" })
        </p>
    </button>
}