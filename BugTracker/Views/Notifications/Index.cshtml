@model IEnumerable<BugTracker.Models.TicketNotification>

@{
    ViewBag.Title = "Index";
}

<h2>Notifications</h2>

<table class="table">
    <tr>
        <th></th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Content)
        </td>
        <td>
            <a href='@Url.Action("Details", "Tickets", new { id = item.TicketId })'>Details</a>
        </td>
    </tr>
}

</table>
